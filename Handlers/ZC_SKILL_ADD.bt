typedef struct
{
    uint64 skillObjectId;
    uint32 skillId;
    uint16 propSize;
    uint16 unk4;
    uint32 unk5;
    uint32 unk6;
} SKILL <optimize=false>;

local uint ZC_SKILL_ADD_CurPos;
